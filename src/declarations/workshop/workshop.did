type State = 
 record {
   events: List;
   value: Entity;
 };
type List = 
 opt record {
       Event;
       List;
     };
type Event = variant {initEvent: record {value: Entity;};};
type Entity = nat;
type Command = variant {initCommand: record {value: Entity;};};
service : {
  action: (Command) -> (opt State);
}
